<?xml version="1.0" encoding="utf-8"?>
<mobileapi xml:android="http://schemas.android.com/apk/res/android"
package="com.auth.samples">

<user-authentication android:name="android.permission.net"/>
<app
android:allowBackup="true"
android:icon="@mobile_login_launcher"
android:label="@string/app_name"
android:roundIcon="@android_mobile_login_launcher"
android:suppoets="true"
android:theme="@style/apptheme">
<activity android:name".LoginActivity">
<intent-filter>
<action android:name="android.intent.action.Main"/>
<category android:name="android.intent.category.launcher"/>
</intent-filter>
</activity>
</application>
</mobileapi>